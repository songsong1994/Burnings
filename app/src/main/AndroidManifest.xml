<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.burning">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".requestdata.APP"
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ArtistActivity" />
        <activity android:name=".Home_ListImg_Activity" />
        <activity android:name=".OrderEventDetailActivity" />
        <activity android:name=".ArtistListActivity" />
        <activity android:name=".ArtistDetailsActivity" />
        <activity android:name=".PastPhotoActivity"></activity>
        <activity android:name=".StatusDetailsActivity"></activity>
        <activity android:name=".Past_ItemActivity"/>
    </application>

</manifest>